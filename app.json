{
    "expo": {
        "name": "Impact Market",
        "slug": "impact-market",
        "privacy": "public",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "0.0.1-alpha.20",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "scheme": "impactmarketmobile",
        "splash": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#2362FB"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "notification": {
            "icon": "./assets/images/notification.png",
            "color": "#000000",
            "iosDisplayInForeground": true,
            "androidMode": "collapse",
            "androidCollapsedTitle": "#{unread_notifications} new interactions"
        },
        "ios": {
            "googleServicesFile": "./assets/firebase/GoogleService-Info.plist",
            "bundleIdentifier": "com.impactmarket.mobile",
            "buildNumber": "0.0.1a20",
            "supportsTablet": true
        },
        "android": {
            "useNextNotificationsApi": true,
            "googleServicesFile": "./assets/firebase/google-services.json",
            "package": "com.impactmarket.mobile",
            "versionCode": 20
        },
        "description": "Decentralized impact-driven 2-sided marketplace to provide financial services to charities and vulnerable beneficiaries.",
        "githubUrl": "https://github.com/impactMarket/mobile-app"
    }
}