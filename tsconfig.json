{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "assets": ["./src/assets"],
            "components": ["./src/components"],
            "helpers": ["./src/helpers"],
            "navigator": ["./src/navigator"],
            "services": ["src/helpers/reduxces"],
            "styles": ["./src/styles"],
            "views": ["./src/views"]
        },
        "strict": true,
        "sourceMap": true,
        "inlineSources": true
    },
    "exclude": ["./node_modules/**/*"],
    "extends": "expo/tsconfig.base"
}
